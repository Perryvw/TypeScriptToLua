{
    "compilerOptions": {
        "outDir": "../../dist/lualib",
        "target": "esnext",
        "lib": ["esnext"],
        "types": ["../../helpers"],
        "skipLibCheck": true,

        "noUnusedLocals": true,
        "noUnusedParameters": true
